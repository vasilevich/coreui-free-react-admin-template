{
  "name": "@coreui-typescript",
  "version": "2.0.0",
  "description": "Simba client based on CoreUI template",
  "author": "Yosef Langer",
  "homepage": "https://simbaems.com",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:coreui/coreui-free-react-admin-template.git"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./node_modules ./src/scss -o ./src/scss",
    "watch-css": "node-sass-chokidar --include-path ./node_modules ./src/scss -o ./src/scss --watch --recursive",
    "watch-ts": "react-scripts-ts start",
    "watch": "npm-run-all -p watch-css watch-ts",
    "start": "npm-run-all -s build-css watch",
    "build-ts": "react-scripts-ts build",
    "build": "npm-run-all build-css build-ts",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "dependencies": {
    "@coreui/coreui": "^2.0.2",
    "@coreui/coreui-plugin-chartjs-custom-tooltips": "^1.2.0",
    "@coreui/icons": "^0.2.0",
    "@coreui/react": "^2.0.1",
    "bootstrap": "^4.1.1",
    "chart.js": "^2.7.2",
    "classnames": "^2.2.6",
    "core-js": "^2.5.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "flag-icon-css": "^3.0.0",
    "font-awesome": "^4.7.0",
    "prop-types": "^15.6.1",
    "react-chartjs-2": "^2.7.2",
    "react-loadable": "^5.4.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.4.0",
    "reactstrap": "^6.1.0",
    "simple-line-icons": "^2.4.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "react-scripts-ts": "2.16.0",
    "babel-jest": "^23.0.1",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.2",
    "@types/jest": "^23.0.2",
    "@types/node": "^10.3.2",
    "@types/react": "^16.3.17",
    "@types/react-dom": "^16.0.6",
    "typescript": "^2.9.1"
  },
  "bugs": {
    "url": "https://git.doar.systems/simba/client/issues"
  }
}
